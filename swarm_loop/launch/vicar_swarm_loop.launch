<launch>
    <node name="swarm_loop" pkg="swarm_loop" type="swarm_loop_node" output="screen">
        <rosparam>
            camera_config_path: /home/xuhao/swarm_ws/src/VINS-Fusion-gpu/config/vi_car/cam0_mei.yaml
            debug_image : true
            lcm_uri: udpm://224.0.0.251:7667?ttl=1
            #lcm_uri: udpm://239.255.76.67:7667?ttl=1
            self_id: 0
        </rosparam>
        <remap from="~left_camera" to="/cam0/image_raw"/>
    </node>
    <node name="vins_estimator" pkg="vins" type="vins_node" output="log" args="/home/xuhao/swarm_ws/src/VINS-Fusion-gpu/config/vi_car/vi_car.yaml" />
    
    <node pkg="localization_proxy" name="localization_proxy" type="localization_proxy_node" output="screen">
       <param name="vins_topic" value="/vins_estimator/imu_propagate" type="string" />
       <rosparam>
            send_fused_freq: 0.0
            send_fused_basecoor_freq: 30.0
            self_id: 0
            enable_uwb: false
       </rosparam>
    </node>
</launch>

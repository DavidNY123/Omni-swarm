<launch>
    <arg name="rand" default="10.0" />
    <arg name="num" default="5" />
    <arg name="detection" default="true" />
    <node pkg="swarm_localization" name="swarm_localization" type="swarm_localization_node" output="screen">
        <rosparam>
            force_freq: 1
            max_accept_cost: 10
            max_keyframe_num: 10
            min_keyframe_num: 5
            thread_num: 4
        </rosparam>
        <param name="swarm_nodes_config" value="$(find swarm_localization)/config/swarm_nodes_test.yaml" type="string" />
        <param name="initial_random_noise" value="$(arg rand)" type="double" />
    </node>
    <node pkg="swarm_localization" name="test_vo_datagen" type="test_vo_datagen.py" output="screen">
        <param name="drone_num" value="$(arg num)" type="int" />
        <param name="detection" value="$(arg detection)" type="bool" />
    </node>
</launch>
